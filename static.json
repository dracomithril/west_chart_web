{
  "root": "build/",
  "routes": {
    "/icons/**": "/icons/",
    "/manifest.json": "/manifest.json",
    "/pic/**": "/pic/",
    "/**": "index.html"
  },
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=7776000"
    }
  },
  "proxies": {
    "/api/": {
      "origin": "https://${API_APP_NAME}.herokuapp.com/"
    }
  }
}
